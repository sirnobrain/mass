<!DOCTYPE html>
<html lang="en">
<head>
  <% include ../partials/head-content %>
</head>
<body>

  <% include ../partials/navigation %>

  <div class="container">
    <h1>Generate Report</h1>
    <form action="/admin/report/generate" method="post">

      <div class="row">
        <div class="input-field col s6">
          <input name="from" id="from" type="text" class="datepicker">
          <label for="from">From</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s6">
          <input name="to" id="to" type="text" class="datepicker">
          <label for="to">To</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s6">
          <button type="submit" class="waves-effect waves-light btn">Generate</button>
        </div>
      </div>

    </form>
  </div>


  <% include ../partials/scripts %>

</body>
</html>
